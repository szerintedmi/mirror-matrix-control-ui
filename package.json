{
    "name": "mirror-array-control-and-config",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "packageManager": "yarn@4.10.3",
    "engines": {
        "node": ">=22.21.0 <23"
    },
    "scripts": {
        "dev": "vite",
        "prebuild": "tsc --noEmit -p tsconfig.json",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "typecheck": "tsc --noEmit -p tsconfig.json",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
        "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
        "test:e2e": "playwright test",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:ui": "playwright test --ui"
    },
    "dependencies": {
        "@babylonjs/core": "8.36.1",
        "mqtt": "5.14.1",
        "react": "19.2.0",
        "react-dom": "19.2.0"
    },
    "devDependencies": {
        "@playwright/test": "1.56.1",
        "@types/eslint-plugin-jsx-a11y": "6",
        "@types/jsdom": "27",
        "@types/node": "22.18.13",
        "@types/react": "19.2.2",
        "@types/react-dom": "19.2.2",
        "@typescript-eslint/eslint-plugin": "8.46.2",
        "@typescript-eslint/parser": "8.46.2",
        "@vitejs/plugin-react": "5.1.0",
        "eslint": "9.39.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.1",
        "globals": "16.4.0",
        "jsdom": "27.1.0",
        "prettier": "3.6.2",
        "typescript": "5.9.3",
        "vite": "7.1.12",
        "vite-plugin-checker": "0.7.2",
        "vitest": "2.1.4"
    }
}
